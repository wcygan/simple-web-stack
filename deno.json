    },
    "imports": {
        "@std/cli/parse-args": "jsr:@std/cli@^0.224.0/parse-args",
        "@std/fmt/colors": "jsr:@std/fmt@^0.224.0/colors",
        "@std/fs/ensure-dir": "jsr:@std/fs@^0.224.0/ensure-dir",
        "@david/dax": "jsr:@david/dax@^0.39.2"
    }
}